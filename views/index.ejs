<p>And here are some users from localhost:</p>
<div id="results"></div>
<script>
  jQuery(function() {
    $.getJSON('http://localhost:3000/api/v3/users?callback=?', function(data) {
      $('#results').html('<ul>' +
          data.map(function(item) {
            return '<li>' + item.user.user_id + '</li>';
          }).join('') + '</ul>');
    });
  });
</script>